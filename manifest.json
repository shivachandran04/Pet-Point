{
    "manifest_version": 3,
    "name": "VW Autofill Extension",
    "version": "1.0",
    "description": "Autofills data from a local file into a website",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "/popup.html"
    }
}
  